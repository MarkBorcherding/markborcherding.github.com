# A sample Gemfile
source "https://rubygems.org"


# Jekyll and deps
gem "jekyll"
gem "redcarpet"
gem 'gsl'

# Development web server
gem 'sinatra'
gem 'sinatra-index'

# CSS Gems
gem 'compass'
gem 'zurb-foundation'


# Guard a few things
gem 'rb-fsevent', '~> 0.9.1'
gem 'guard-jekyll'
gem 'guard-compass'
gem 'guard-livereload'
gem 'yajl-ruby'
